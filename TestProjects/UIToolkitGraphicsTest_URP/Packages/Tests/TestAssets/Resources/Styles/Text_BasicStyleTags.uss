Label {
    -unity-font-definition: url("../Fonts & Materials/LiberationSans SDF.asset");
    font-size: 50px;
    color: white;
    margin: 0px;
    padding: 0px 2px 0px 1px;
}

.background {
    flex-grow: 1;
    background-color: #2e6da4;
}

.container {
    flex-grow: 1;
    flex-direction: row;
    justify-content: space-between;
    margin-left: 20px;
    margin-right: 20px;
    margin-top: 20px;
}

.multiline {
    width: 550px;
    white-space: normal;
}

.title {
    font-size: 100px;
    justify-content: center;
    align-items: center;
    -unity-text-align: upper-center;
    text-shadow: 3px 3px 4px black;
}
